{
  "name": "camelsnake",
  "version": "1.0.0",
  "description": "Convert object keys between camel-case and snake-case",
  "main": "index.js",
  "repository": "https://github.com/nino/camelsnake",
  "author": "Nino AnnighÃ¶fer <nino@ninoan.com>",
  "license": "MIT",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build:clean": "rimraf dist",
    "build:babel": "babel src --out-dir=dist --ignore test.js",
    "build:flow": "flow-copy-source -i '**/__test__/**' src dist",
    "build": "yarn build:clean && yarn build:babel && yarn build:flow",
    "test": "ava",
    "check": "eslint src",
    "precommit": "lint-staged && yarn test",
    "prepush": "lint-staged && yarn test",
    "prepublish": "yarn check && yarn test && yarn build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "eslint": {
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "extends": [
      "prettier/prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ],
      "flow"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.12.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "flow-bin": "^0.60.1",
    "flow-copy-source": "^1.2.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
